:set prompt "\ESC[38;5;105m Î» \ESC[m"
:set editor subl
:set -XOverloadedStrings

let replaceBash '$' = "\\$"
let replaceBash '"' = "\\\""
let replaceBash '\'' = "\\'"
let replaceBash x = x
let escapeBash x = concat . map replaceBash $ x

:def pf \str -> return $ ":! pointfree \"" ++ str ++ "\""

:def hoogle \str -> return $ ":! hoogle \"" ++ str ++ "\""
